{
  "name": "@descope/web-js-sdk",
  "version": "1.8.0",
  "author": "Descope Team <info@descope.com>",
  "homepage": "https://github.com/descope/web-js-sdk",
  "bugs": {
    "url": "https://github.com/descope/web-js-sdk/issues",
    "email": "help@descope.com"
  },
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/cjs/index.cjs.js"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.esm.js"
    }
  },
  "type": "module",
  "description": "Descope JavaScript web SDK",
  "scripts": {
    "start": "npm run generateCerts && npx nx run web-js-sdk:build && npm run build && http-server -S -o ./examples",
    "build": "rimraf dist && rollup -c",
    "test": "jest",
    "lint": "eslint '+(src|test|examples)/**/*.ts'",
    "generateCerts": "test -f ./key.pem && test -f ./cert.pem || openssl req -new -newkey rsa:2048 -days 365 -nodes -x509 -subj '/C=US/ST=California/L=San Francisco/O=Descope/CN=localhost' -keyout ./key.pem -out ./cert.pem"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/descope/web-js-sdk.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "descope",
    "authentication"
  ],
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "1.2.5",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "18.18.4",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.get": "^4.4.7",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "8.52.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "http-server": "^14.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-browsersync": "^1.3.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "ts-jest": "^29.0.0",
    "ts-node": "10.9.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@descope/core-js-sdk": "workspace:*",
    "@fingerprintjs/fingerprintjs-pro": "3.8.5",
    "js-cookie": "3.0.5",
    "tslib": "2.6.2"
  },
  "overrides": {
    "terser": "^5.14.2"
  }
}
