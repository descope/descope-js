{
  "name": "web-js-sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/sdks/web-js-sdk/src",
  "projectType": "library",
  "targets": {
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "trackDeps": true,
        "push": false,
        "preset": "conventional"
      }
    },
    "license": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tools/scripts/licenseValidation/thirdPartyLicenseCollector_linux_amd64 -npm-project {projectRoot} -go-project tools/scripts/licenseValidation"
        ]
      }
    }
  },
  "tags": []
}
